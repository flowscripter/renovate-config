{
    "name": "@flowscripter/renovate-config",
    "version": "0.0.0",
    "description": "Common Renovate config for all Flowscripter projects.",
    "repository": {
        "type": "git",
        "url": "https://github.com/flowscripter/renovate-config"
    },
    "homepage": "https://github.com/flowscripter/renovate-config",
    "bugs": {
        "url": "https://github.com/flowscripter/renovate-config/issues"
    },
    "keywords": [
        "renovate"
    ],
    "contributors": [
        {
            "name": "Vectronic",
            "email": "hello.vectronic@gmail.com",
            "url": "https://vectronic.io"
        }
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@commitlint/cli": "9.0.1",
        "@commitlint/config-conventional": "9.0.1",
        "husky": "4.2.5",
        "semantic-release": "17.1.1"
    },
    "peerDependencies": {},
    "dependencies": {},
    "renovate-config": {
        "default": {
            "extends": [
                "config:base",
                "default:automergePatch",
                "default:automergeMinor"
            ],
            "packageRules": [
                {
                    "depTypeList": [
                        "devDependencies"
                    ],
                    "groupName": "devDependencies"
                }
            ]
        }
    }
}
